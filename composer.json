{
    "name": "laravel/project",
    "description": "Laravel Project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "dergahab/arch-testing": "*",
        "pestphp/pest": "^2.35"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dergahab/arch-testing.git"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "./vendor/bin/pest"
        ]
    },
    "authors": [
        {
            "name": "Dergah"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
